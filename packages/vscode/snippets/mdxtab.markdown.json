{
  "MDXTab frontmatter": {
    "prefix": "mdxtab-frontmatter",
    "body": [
      "---",
      "mdxtab: \"1.0\"",
      "tables:",
      "  ${1:table}:",
      "    key: ${2:id}",
      "    columns: [${2:id}, ${3:col}]",
      "    computed:",
      "      ${4:computed}: ${5:expr}",
      "    aggregates:",
      "      ${6:total}: sum(${3:col})",
      "---"
    ],
    "description": "Insert MDXTab frontmatter scaffold."
  },
  "MDXTab table": {
    "prefix": "mdxtab-table",
    "body": [
      "## ${1:table}",
      "",
      "| ${2:col1} | ${3:col2} |",
      "|---|---|",
      "| ${4} | ${5} |"
    ],
    "description": "Insert a basic MDXTab table scaffold."
  }
}
